# Required: Google Gemini API key for AI features
GEMINI_API_KEY=your_gemini_api_key_here

# Optional: E2B API key for cloud code execution (if not provided, will use Docker)
E2B_API_KEY=your_e2b_api_key_here

# Optional: Gemini model to use (default: gemini-1.5-flash)
GEMINI_MODEL=gemini-1.5-flash

# Optional: Server port (default: 3001)
PORT=3001

# Optional: Frontend URL for CORS (default: http://localhost:5173)
FRONTEND_URL=http://localhost:5173 